{
	"name": "@musicplayce/ts-sdk",
	"version": "0.0.10",
	"main": "dist/index",
	"typings": "dist/index.d.ts",
	"author": "Gustavo Quinta <gustavoqnt40@gmail.com>",
	"description": "MusicPlayce SDK made with Typescript intended for client & server-side use.",
	"license": "MIT",
	"publishConfig": {
		"access": "public"
	},
	"keywords": [
		"typescript",
		"nodejs",
		"eslint",
		"boilerplates",
		"tsconfig",
		"musicplayce",
		"ts-sdk"
	],
	"homepage": "https://github.com/musicplayce/ts-sdk",
	"bugs": {
		"url": "https://github.com/musicplayce/ts-sdk/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/musicplayce/ts-sdk.git"
	},
	"scripts": {
		"cz": "npx git-cz",
		"prepublishOnly": "npm run test && npm run build",
		"build": "npm run clean && tsc -p .",
		"build:watch": "npm run clean & tsc -watch",
		"lint": "eslint lib/**/*.ts",
		"lint:fix": "prettier-eslint lib/**/*.ts --write",
		"test": "mocha -r ts-node/register tests/**/*.test.ts",
		"release": "npx standard-version",
		"clean": "rm -rf ./dist"
	},
	"devDependencies": {
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@types/chai": "^4.2.11",
		"@types/lodash": "^4.14.150",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.13.4",
		"@types/qs": "^6.9.1",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"chai": "^4.2.0",
		"cz-conventional-changelog": "3.1.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-json": "^2.1.1",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^4.2.5",
		"mocha": "^7.1.2",
		"prettier": "^2.0.5",
		"prettier-eslint": "^9.0.1",
		"prettier-eslint-cli": "^5.0.0",
		"rimraf": "^3.0.2",
		"standard-version": "^8.0.1",
		"ts-node": "^8.10.1",
		"typescript": "^3.8.3"
	},
	"dependencies": {
		"axios": "^0.19.2",
		"axios-rate-limit": "^1.2.1",
		"lodash": "^4.17.15",
		"qs": "^6.9.4"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	},
	"husky": {
		"hooks": {
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	}
}
